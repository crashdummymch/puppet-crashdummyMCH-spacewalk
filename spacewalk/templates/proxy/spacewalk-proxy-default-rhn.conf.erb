# File managed by Puppet  on <%= scope.lookupvar('nodeclassify::puppet_server') %>
#
# Automatically generated RHN Proxy Server configuration file.
# -------------------------------------------------------------------------

# SSL CA certificate location
proxy.ca_chain = <%= @proxy_ca_chain %>

# Corporate HTTP proxy, format: corp_gateway.example.com:8080
proxy.http_proxy = <%= @proxy_http_proxy %>

# Username for that corporate HTTP proxy
proxy.http_proxy_username = <%= @proxy_http_proxy_username %>

# Password for that corporate HTTP proxy
proxy.http_proxy_password = <%= @proxy_http_proxy_password %>

proxy.proxy_local_flist = <%= @proxy_proxy_local_flist %>

# Location of locally built, custom packages
proxy.pkg_dir = <%= @proxy_pkg_dir %>

# Hostname of RHN Server or RHN Satellite
proxy.rhn_parent = <%= @proxy_rhn_parent %>

# Destination of all tracebacks, etc.
traceback_mail = <%= @traceback_mail %>

# SSL eabled or disabled
proxy.use_ssl = <%= @proxy_use_ssl %>

# Monitoring Scout enabled or disabled
web.is_monitoring_scout = <%= @web_is_monitoring_scout %>
